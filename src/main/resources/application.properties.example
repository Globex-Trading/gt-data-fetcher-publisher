spring.data.mongodb.uri=mongodb://localhost:27017/mde

# SSL/TLS Configuration
server.port=8080
server.ssl.certificate=classpath:certs/fullchain.pem
server.ssl.certificate-private-key=classpath:certs/privkey.pem

#Custom Configuration

#CORS Policy
custom.cors.allowed_urls=http://localhost

#Asyncronous Method Handling + Thread Pool
custom.concurrency.async.config.core_pool_size=20
custom.concurrency.async.config.max_pool_size=2147480000

#Node GT-API URL
custom.gt-api.hostname=http://localhost:3000